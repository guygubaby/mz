{
  "name": "@bryce-loskie/mz",
  "version": "0.1.4",
  "description": "A zoom image component for vue3 using medium-zoom",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "pnpm build",
    "dev": "pnpm build -- --watch",
    "build": "tsup"
  },
  "keywords": [
    "medium-zoom",
    "image viewer"
  ],
  "author": "guygubaby@gmail.com",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guygubaby/mz.git"
  },
  "dependencies": {
    "medium-zoom": "^1.0.6"
  }
}
